<?xml version="1.0" encoding="UTF-8" ?>   
<!DOCTYPE mapper   
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    <mapper namespace='com.jiekou.sj.QuestionnaireDao'>
		<insert id="insertQ" parameterType="com.model.sj.Questionnaire">
			INSERT INTO questionnaire (contentdesign,method,expressiveability,course,organizationw,lecture,facilities,ability,evaluate,surveyratio)VALUES(#{contentdesign},#{method},#{expressiveability},#{course},#{organizationw},#{lecture},#{facilities},#{ability},#{evaluate},#{surveyratio})
		</insert>
    </mapper>